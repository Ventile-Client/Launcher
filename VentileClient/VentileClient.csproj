  del *.xml
  echo Deleted .xml files
  del *.pdb
  echo Deleted .pdb files
  del *.winmd
  echo Deleted .winmd files
  powershell Compress-Archive -Path "$(TargetDir)LauncherZip\*.*" -DestinationPath "$(TargetDir)\VentileClient.zip"
